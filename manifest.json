{
"manifest_version": 2,
	"name": "MaterialDeck",
	"version": "1.0.1",
	"author": "[MEADOW_DEV]",
	"description": "MaterialDeck is an extension for Google Chrome/Opera that enhances the appearance of TweetDeck using Google's Material Design",
	"icons":{
		"16":"img/icon-1024.png",
		"32":"img/icon-1024.png",
		"48":"img/icon-1024.png",
		"128":"img/icon-1024.png"
	},
	"permissions": [
		"https://tweetdeck.twitter.com/*"
	],
	"background": {
		"scripts": ["js/background.js"]
	},
	"web_accessible_resources": [
		"fonts/*",
		"img/*",
		"css/*",
		"js/*"
	],
	"content_scripts": [
			{
			 "matches": ["https://tweetdeck.twitter.com/*"],
			 "js": ["js/jquery-2.1.3.min.js", "js/contentscript.js"],
			 "run_at" : "document_start"
			}
	]
}
